<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="DoubleSeekBar">
        <item name="DB_ThumbRadius">12dp</item>
        <item name="DB_ThumbColor">#FFFFFF</item>
        <item name="DB_ProgressColor">#57cad1</item>
        <item name="DB_ProgressBackgroundColor">#1f000000</item>
        <item name="DB_ProgressWidth">2dp</item>
        <item name="DB_TextSize">15sp</item>
        <item name="DB_TextColor">#303030</item>
    </style>

</resources>
